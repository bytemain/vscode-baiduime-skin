{
  "name": "css",
  "scopeName": "source.css",
  "fileTypes": [".css"],
  "uuid": "a562f074-795e-4251-9441-ec0d13e09e98",
  "patterns": [
    {
      "comment": "Comments - starts with #",
      "match": "(#).*$\\n?",
      "name": "comment.line.number-sign.css",
      "captures": {
        "1": {
          "name": "punctuation.definition.comment.css"
        }
      }
    },
    {
      "comment": "Keywords",
      "match": "\\b([a-zA-Z0-9_.-]+)\\b\\s*(=)",
      "captures": {
        "1": {
          "name": "keyword.other.definition.css"
        },
        "2": {
          "name": "punctuation.separator.key-value.css"
        }
      }
    },
    {
      "comment": "Assignment Operator",
      "match": "(?<=[\\w])\\s?=",
      "name": "keyword.operator.assignment.css"
    },
    {
      "comment": "Numeric",
      "match": "\\b((0(x|X)[0-9a-fA-F]*)|(([0-9]+\\.?[0-9]*)|(\\.[0-9]+))((e|E)(\\+|-)?[0-9]+)?)\\b",
      "name": "constant.numeric.css"
    },
    {
      "match": "^(\\[)(.*?)(\\])",
      "name": "entity.name.section.group-title.css",
      "captures": {
        "1": {
          "name": "punctuation.definition.entity.css"
        },
        "3": {
          "name": "punctuation.definition.entity.css"
        }
      }
    }
  ]
}
